page{
  
}
/* 题号部分 */

.main {
  margin: 20rpx;
  font-size: 34rpx;
}

/* 问题部分 */

.question {
  display: block;
  /* height: 150rpx; */
  padding: 27rpx 30rpx 36rpx;
 font-size: 30rpx;
  background: #fff;
}

.xuanz {
  display: inline-block;
  margin: -5rpx 0 0;
  border-radius: 10rpx;
  font-size: 24rpx;
  /* border: 1rpx solid #ccc; */
  width: 90rpx;
  text-align: center;
  line-height: 34rpx;
  vertical-align: middle;
  background-color: #1296DB;
  color: white;
}

/* 题  目 */

.question .question_name {
  /* display: flex; */
font-size: 28rpx;
  margin-top: 10rpx;
}

.question_item1, .question_item2, .question_item3 {
  margin: 20rpx;
  font-size: 30rpx;
  /* border-bottom: 1px solid gainsboro; */
}

/* 下一题 */

.next {
  background: #1bd0bd;
  border-radius: 50rpx;
  width: 300rpx;
  height: 80rpx;
  font-size: 32rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: auto;
  color: white;
  margin-top: 100rpx;
}

.css-shadow {
  -moz-box-shadow: 2px 2px 10px #2db7f5;
  -webkit-box-shadow: 2px 2px 10px #2db7f5;
  box-shadow: 2px 2px 10px #2db7f5;
}

/* 答案解析 */

.analysis {
  line-height: 60rpx;
  width: 100%;
  height: 60rpx;
  margin-top: 100rpx;
  margin-bottom:30rpx;
  background: #fff;
  border-radius: 10rpx;
  padding: 20rpx;
  font-size: 30rpx;
}
.answer_txt{
  color: #FF0000;
}
.jd-txt {
  border-radius: 5px;
  border: 1px solid gainsboro;
  box-shadow: 0px 2px 5px 5px gainsboro;
  margin-top: 12rpx;
   font-size: 30rpx;
}

.txt {
  margin-left: 5rpx;
  line-height: 60rpx;
}

/* 底部菜单 */

.placeholder {
  bottom: 0px;
  margin: 5px;
  padding: 0 10px;
  text-align: center;
  height: 2.3em;
  line-height: 2.3em;
}

.item-store {
  width: 14px;
  height: 14px;
}

.item-error {
  width: 14px;
  height: 14px;
}
.bo-txt{
  text-align: center;
  justify-items: center;
  font-size: 12px;
  color: gainsboro;
}




/* sss */
.footer-layer {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
}

.footer {
  position: fixed;
  box-sizing: border-box;
  bottom: 0;
  width: 100%;
  transform: translate3d(0, 100%, 0px);
  z-index: 100;
}

.count {
  position: absolute;
  box-sizing: border-box;
  top: -93rpx;
  width: 100%;
  padding: 31rpx 30rpx;
  background: #fff;
  font-size: 24rpx;
  overflow: hidden;
  zoom: 1;
  z-index: 1000;
}

.count view {
  float: right;
  line-height: 34rpx;
}

.count .collect {
  float: left;
  height: 34rpx;
  padding-left: 45rpx;
  background: url("data:image/png;base64,") no-repeat left center/34rpx 34rpx;
  font-size: 28rpx;
  color: #666;
  line-height: 34rpx;
}

.count .collect.active {
  background: url("data:image/png;base64,") no-repeat left center/34rpx 34rpx;
}

.count .page {
  height: 30rpx;
  padding: 0 0 0 37rpx;
  font-size: 24rpx;
  color: #999;
  background: url("data:image/png;base64,") no-repeat left center/28rpx 28rpx;
  line-height: 36rpx;
}

.count .error {
  height: 30rpx;
  margin-right: 30rpx;
  padding-left: 40rpx;
  background: url("data:image/png;base64,") no-repeat left center/30rpx 30rpx;
  font-size: 24rpx;
  color: #f66;
  vertical-align: middle;
  line-height: 36rpx;
}

.count .success {
  height: 30rpx;
  margin-right: 30rpx;
  padding-left: 40rpx;
  background: url("data:image/png;base64,") no-repeat left center/30rpx 30rpx;
  font-size: 24rpx;
  color: #6c0;
  vertical-align: middle;
  line-height: 36rpx;
}

.clear {
  zoom: 1;
  overflow: hidden !important;
}

.status-lists {
  max-height: 500rpx;
  padding: 0 0 24rpx;
  border-top: 1rpx solid #eaeaea;
  background: #fff;
  z-index: 100;
  overflow: auto;
}

.status-lists .status-list {
  float: left;
  width: 98rpx;
  height: 98rpx;
  margin: 24rpx 0 0 20rpx;
  border-radius: 50rpx;
  border: 1rpx solid #ccc;
  font-size: 30rpx;
  line-height: 98rpx;
  text-align: center;
  color: #ccc;
}

.status-lists .status-list.active {
  color: #999;
  border-color: #999;
  background: #f6f6f6;
}

.status-lists .status-list.error {
  color: #f66;
  border-color: #f66;
}

.status-lists .status-list.success {
  color: #6c0;
  border-color: #6c0;
}

.status-lists .status-list.error.active {
  color: #f66;
  border-color: #f66;
  background: #fff3f3;
}

.status-lists .status-list.success.active {
  color: #6c0;
  border-color: #6c0;
  background: #f7fff0;
}